--  Programs used to test gnatfmt

with "libadalang";
with "gnatfmt";
with "gnatfmt_common";

project Test_Programs is
   for Source_Dirs use ("../src/gendoc");
   for Object_Dir use "../obj/test-programs";
   for Exec_dir use "../bin";
   for Main use ("gendoc.adb");

   package Compiler renames Gnatfmt_Common.Compiler;

   package Binder is
      for Default_Switches ("Ada") use ("-Es");
   end Binder;
end Test_Programs;
